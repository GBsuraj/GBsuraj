      "source": [
        "#multiplication of matrix\n",
        "x=[[11,21,31],\n",
        "   [12,22,32]]\n",
        "y=[[1,4],\n",
        "   [2,5],\n",
        "   [3,6]]\n",
        "result=[[0,0],\n",
        "        [0,0]]\n",
        "for i in range(len(x)):\n",
        "  for j in range(len(y[0])):\n",
        "    for k in range(len(y)):\n",
        "      result[i][j]+=x[i][k]*y[k][j]\n",
        "for r in result:\n",
        "  print(r)"
      ],

    "class bird:\n",
    "    def __init__(self,name,colour):\n",
    "        self.name=name\n",
    "        self.colour=colour\n",
    "    def Bird_size(self,size):\n",
    "        return f\"The {self.name} is {self.colour} in colour and size is {size}\"\n",
    "birds= bird('Parrot','green')\n",
    "print(birds.Bird_size(\"medium\"))\n"
   ]
